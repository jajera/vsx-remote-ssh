{
  "name": "vsx-remote-ssh",
  "displayName": "Remote SSH",
  "description": "Connect to and develop on remote servers via SSH",
  "version": "1.0.10",
  "publisher": "jajera",
  "repository": {
    "type": "git",
    "url": "https://github.com/jajera/vsx-remote-ssh.git"
  },
  "bugs": {
    "url": "https://github.com/jajera/vsx-remote-ssh/issues"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "license": "MIT",
  "homepage": "https://github.com/jajera/vsx-remote-ssh",
  "galleryBanner": {
    "color": "#1e415e",
    "theme": "dark"
  },
  "icon": "resources/icon.png",
  "categories": [
    "Other",
    "Remote Development"
  ],
  "keywords": [
    "ssh",
    "remote",
    "development",
    "server",
    "sftp"
  ],
  "activationEvents": [
    "onCommand:remote-ssh.connect",
    "onCommand:remote-ssh.addHost",
    "onCommand:remote-ssh.disconnect",
    "onCommand:remote-ssh.reconnect",
    "onCommand:remote-ssh.showConnections",
    "onCommand:remote-ssh.manageHosts",
    "onCommand:remote-ssh.editHost",
    "onCommand:remote-ssh.deleteHost",
    "onCommand:remote-ssh.setDefaultHost",
    "onCommand:remote-ssh.testConnection",
    "onCommand:remote-ssh.openTerminal",
    "onCommand:remote-ssh.closeTerminals",
    "onCommand:remote-ssh.openWorkspace",
    "onCommand:remote-ssh.switchWorkspace",
    "onCommand:remote-ssh.showHostInfo",
    "onCommand:remote-ssh.showCacheStatistics",
    "onCommand:remote-ssh.clearCache",
    "onCommand:remote-ssh.exportConfiguration",
    "onCommand:remote-ssh.importConfiguration",
    "onCommand:remote-ssh.mountFolder",
    "onCommand:remote-ssh.unmountFolder",
    "onCommand:remote-ssh.manageMounts",
    "onCommand:remote-ssh.refreshMount",
    "onCommand:remote-ssh.test"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "remote-ssh.connect",
        "title": "Connect to Host via SSH",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.addHost",
        "title": "Add SSH Host",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.disconnect",
        "title": "Disconnect",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.reconnect",
        "title": "Reconnect",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.showConnections",
        "title": "Show Active Connections",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.manageHosts",
        "title": "Manage SSH Hosts",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.editHost",
        "title": "Edit SSH Host",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.deleteHost",
        "title": "Delete SSH Host",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.setDefaultHost",
        "title": "Set Default Host",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.testConnection",
        "title": "Test SSH Connection",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.openTerminal",
        "title": "Open Remote Terminal",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.closeTerminals",
        "title": "Close All Terminals",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.openWorkspace",
        "title": "Open Remote Workspace",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.switchWorkspace",
        "title": "Switch Workspace",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.showHostInfo",
        "title": "Show Host Information",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.showCacheStatistics",
        "title": "Show Cache Statistics",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.clearCache",
        "title": "Clear Cache",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.exportConfiguration",
        "title": "Export Configuration",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.importConfiguration",
        "title": "Import Configuration",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.mountFolder",
        "title": "Mount Remote Folder",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.unmountFolder",
        "title": "Unmount Remote Folder",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.manageMounts",
        "title": "Manage Mounted Folders",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.refreshMount",
        "title": "Refresh Mounted Folder",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.test",
        "title": "Test SSH Extension",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.contextMenu.mountFolder",
        "title": "Mount Folder",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.contextMenu.connect",
        "title": "Connect",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.contextMenu.disconnect",
        "title": "Disconnect",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.contextMenu.openTerminal",
        "title": "Open Terminal",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.contextMenu.showConnectionInfo",
        "title": "Show Connection Info",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.contextMenu.unmountFolder",
        "title": "Unmount Folder",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.contextMenu.refreshMount",
        "title": "Refresh Mount",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.contextMenu.reconnectMount",
        "title": "Reconnect Mount",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.contextMenu.configureMountOptions",
        "title": "Configure Mount Options",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.contextMenu.openMountTerminal",
        "title": "Open Terminal Here",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.contextMenu.showMountInfo",
        "title": "Show Mount Info",
        "category": "Remote SSH"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "remote-ssh.connections",
          "name": "SSH Connections",
          "when": "true"
        }
      ]
    },
    "menus": {
      "view/item/context": [
        {
          "command": "remote-ssh.contextMenu.mountFolder",
          "when": "view == remote-ssh.connections && viewItem == sshConnection",
          "group": "mount@1"
        },
        {
          "command": "remote-ssh.contextMenu.connect",
          "when": "view == remote-ssh.connections && viewItem == sshConnection",
          "group": "connection@1"
        },
        {
          "command": "remote-ssh.contextMenu.disconnect",
          "when": "view == remote-ssh.connections && viewItem == sshConnection",
          "group": "connection@2"
        },
        {
          "command": "remote-ssh.contextMenu.openTerminal",
          "when": "view == remote-ssh.connections && viewItem == sshConnection",
          "group": "terminal@1"
        },
        {
          "command": "remote-ssh.contextMenu.showConnectionInfo",
          "when": "view == remote-ssh.connections && viewItem == sshConnection",
          "group": "info@1"
        }
      ],
      "explorer/context": [
        {
          "command": "remote-ssh.contextMenu.unmountFolder",
          "when": "resourceScheme == ssh-mount",
          "group": "mount@1"
        },
        {
          "command": "remote-ssh.contextMenu.refreshMount",
          "when": "resourceScheme == ssh-mount",
          "group": "mount@2"
        },
        {
          "command": "remote-ssh.contextMenu.reconnectMount",
          "when": "resourceScheme == ssh-mount",
          "group": "mount@3"
        },
        {
          "command": "remote-ssh.contextMenu.configureMountOptions",
          "when": "resourceScheme == ssh-mount",
          "group": "mount@4"
        },
        {
          "command": "remote-ssh.contextMenu.openMountTerminal",
          "when": "resourceScheme == ssh-mount",
          "group": "terminal@1"
        },
        {
          "command": "remote-ssh.contextMenu.showMountInfo",
          "when": "resourceScheme == ssh-mount",
          "group": "info@1"
        }
      ]
    },
    "configuration": {
      "title": "Remote SSH",
      "properties": {
        "remote-ssh.defaultPort": {
          "type": "number",
          "default": 22,
          "description": "Default SSH port"
        },
        "remote-ssh.connectTimeout": {
          "type": "number",
          "default": 15000,
          "description": "Connection timeout in milliseconds"
        },
        "remote-ssh.reconnectAttempts": {
          "type": "number",
          "default": 3,
          "description": "Number of automatic reconnection attempts"
        }
      }
    }
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vitest run",
    "test:watch": "vitest src",
    "package": "npx @vscode/vsce package",
    "compile:watch": "tsc -watch -p ./",
    "extension:dev": "npm run compile && code --extensionDevelopmentPath=.",
    "extension:test": "npm run compile && code --extensionDevelopmentPath=. --disable-extensions",
    "changelog": "node scripts/generate-changelog.js"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/sinon": "^17.0.4",
    "@types/ssh2": "^1.15.5",
    "@types/ssh2-sftp-client": "^9.0.5",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/vsce": "^2.32.0",
    "chai-sinon": "^2.8.1",
    "eslint": "^8.57.1",
    "markdown-it": "^13.0.1",
    "markdownlint-cli": "^0.45.0",
    "rimraf": "^5.0.1",
    "sinon": "^21.0.0",
    "sinon-chai": "^4.0.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.26.0",
    "typescript": "^5.4.5",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "asn1": "^0.2.6",
    "bcrypt-pbkdf": "^1.0.2",
    "sinon": "^21.0.0",
    "ssh2": "^1.16.0",
    "ssh2-sftp-client": "^9.1.0",
    "uuid": "^11.1.0"
  },
  "bundleDependencies": [
    "ssh2",
    "ssh2-sftp-client",
    "uuid",
    "asn1",
    "bcrypt-pbkdf"
  ],
  "files": [
    "out/**/*",
    "resources/**/*",
    "README.md",
    "CHANGELOG.md",
    "package.json",
    "node_modules/ssh2/**/*",
    "node_modules/ssh2-sftp-client/**/*",
    "node_modules/uuid/**/*",
    "node_modules/asn1/**/*",
    "node_modules/bcrypt-pbkdf/**/*"
  ]
}
