{
  "name": "vsx-remote-ssh",
  "displayName": "Remote SSH",
  "description": "Connect to and develop on remote servers via SSH",
  "version": "1.1.1",
  "publisher": "jajera",
  "repository": {
    "type": "git",
    "url": "https://github.com/jajera/vsx-remote-ssh.git"
  },
  "bugs": {
    "url": "https://github.com/jajera/vsx-remote-ssh/issues"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "license": "MIT",
  "homepage": "https://github.com/jajera/vsx-remote-ssh",
  "galleryBanner": {
    "color": "#1e415e",
    "theme": "dark"
  },
  "icon": "resources/icon.png",
  "categories": [
    "Other",
    "Remote Development"
  ],
  "keywords": [
    "ssh",
    "remote",
    "development",
    "server",
    "sftp"
  ],
  "activationEvents": [
    "*",
    "onCommand:remote-ssh.connect",
    "onCommand:remote-ssh.addHost",
    "onCommand:remote-ssh.disconnect",
    "onCommand:remote-ssh.reconnect",
    "onCommand:remote-ssh.showConnections",
    "onCommand:remote-ssh.manageHosts",
    "onCommand:remote-ssh.editHost",
    "onCommand:remote-ssh.deleteHost",
    "onCommand:remote-ssh.setDefaultHost",
    "onCommand:remote-ssh.testConnection",
    "onCommand:remote-ssh.openTerminal",
    "onCommand:remote-ssh.closeTerminals",
    "onCommand:remote-ssh.openWorkspace",
    "onCommand:remote-ssh.switchWorkspace",
    "onCommand:remote-ssh.showHostInfo",
    "onCommand:remote-ssh.showCacheStatistics",
    "onCommand:remote-ssh.clearCache",
    "onCommand:remote-ssh.exportConfiguration",
    "onCommand:remote-ssh.importConfiguration",
    "onCommand:remote-ssh.test",
    "onCommand:remote-ssh.test-mount-command",
    "onCommand:remote-ssh.mountRemoteFolder",
    "onCommand:remote-ssh.unmountRemoteFolder",
    "onCommand:remote-ssh.showMountPerformanceStats",
    "onCommand:remote-ssh.optimizeMountPerformance"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "remote-ssh.connect",
        "title": "Connect to Host via SSH",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.addHost",
        "title": "Add SSH Host",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.disconnect",
        "title": "Disconnect",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.reconnect",
        "title": "Reconnect",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.showConnections",
        "title": "Show Active Connections",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.manageHosts",
        "title": "Manage SSH Hosts",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.editHost",
        "title": "Edit SSH Host",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.deleteHost",
        "title": "Delete SSH Host",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.setDefaultHost",
        "title": "Set Default Host",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.testConnection",
        "title": "Test SSH Connection",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.openTerminal",
        "title": "Open Remote Terminal",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.closeTerminals",
        "title": "Close All Terminals",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.openWorkspace",
        "title": "Open Remote Workspace",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.switchWorkspace",
        "title": "Switch Workspace",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.showHostInfo",
        "title": "Show Host Information",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.showCacheStatistics",
        "title": "Show Cache Statistics",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.clearCache",
        "title": "Clear Cache",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.exportConfiguration",
        "title": "Export Configuration",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.importConfiguration",
        "title": "Import Configuration",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.test",
        "title": "Test SSH Extension",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.test-mount-command",
        "title": "Test Mount Command",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.mountRemoteFolder",
        "title": "Mount Remote Folder",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.unmountRemoteFolder",
        "title": "Unmount Remote Folder",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.showMountPerformanceStats",
        "title": "Show Mount Performance Stats",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.optimizeMountPerformance",
        "title": "Optimize Mount Performance",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.openTerminalInMount",
        "title": "Open Terminal in Mount",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.openTerminalInCurrentFolder",
        "title": "Open Terminal in Current Folder",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.openTerminalWithPath",
        "title": "Open Terminal with Path",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.initializeGitRepository",
        "title": "Initialize Git Repository",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.gitStatus",
        "title": "Git Status",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.gitAdd",
        "title": "Git Add",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.gitCommit",
        "title": "Git Commit",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.gitPush",
        "title": "Git Push",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.gitPull",
        "title": "Git Pull",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.gitCheckout",
        "title": "Git Checkout",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.gitBranch",
        "title": "Git Branch",
        "category": "Remote SSH"
      },
      {
        "command": "remote-ssh.refreshSourceControl",
        "title": "Refresh Source Control",
        "category": "Remote SSH"
      }
    ],
    "configuration": {
      "title": "Remote SSH",
      "properties": {
        "remote-ssh.defaultPort": {
          "type": "number",
          "default": 22,
          "description": "Default SSH port"
        },
        "remote-ssh.connectTimeout": {
          "type": "number",
          "default": 15000,
          "description": "Connection timeout in milliseconds"
        },
        "remote-ssh.reconnectAttempts": {
          "type": "number",
          "default": 3,
          "description": "Number of automatic reconnection attempts"
        },
        "remote-ssh.mount.cacheSize": {
          "type": "number",
          "default": 50,
          "description": "Cache size for mounted folders in MB"
        },
        "remote-ssh.mount.cacheTTL": {
          "type": "number",
          "default": 300000,
          "description": "Cache TTL for mounted folders in milliseconds"
        },
        "remote-ssh.mount.prefetchEnabled": {
          "type": "boolean",
          "default": false,
          "description": "Enable prefetching for mounted folders"
        },
        "remote-ssh.mount.compressionEnabled": {
          "type": "boolean",
          "default": false,
          "description": "Enable compression for mounted folders"
        },
        "remote-ssh.mount.autoReconnect": {
          "type": "boolean",
          "default": true,
          "description": "Automatically reconnect mounted folders"
        },
        "remote-ssh.mount.watchExcludePatterns": {
          "type": "array",
          "default": ["**/node_modules/**", "**/.git/**"],
          "description": "Patterns to exclude from file watching in mounted folders"
        }
      }
    }
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vitest run",
    "test:watch": "vitest src",
    "package": "npx @vscode/vsce package",
    "compile:watch": "tsc -watch -p ./",
    "extension:dev": "npm run compile && code --extensionDevelopmentPath=.",
    "extension:test": "npm run compile && code --extensionDevelopmentPath=. --disable-extensions",
    "changelog": "node scripts/generate-changelog.js"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/sinon": "^17.0.4",
    "@types/ssh2": "^1.15.5",
    "@types/ssh2-sftp-client": "^9.0.5",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/vsce": "^2.32.0",
    "chai": "^4.3.7",
    "eslint": "^8.57.1",
    "markdown-it": "^13.0.1",
    "markdownlint-cli": "^0.45.0",
    "rimraf": "^5.0.1",
    "sinon-chai": "^3.7.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.26.0",
    "typescript": "^5.4.5",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "asn1": "^0.2.6",
    "bcrypt-pbkdf": "^1.0.2",
    "sinon": "^21.0.0",
    "ssh2": "^1.16.0",
    "ssh2-sftp-client": "^9.1.0",
    "uuid": "^11.1.0"
  },
  "bundleDependencies": [
    "ssh2",
    "ssh2-sftp-client",
    "uuid",
    "asn1",
    "bcrypt-pbkdf"
  ],
  "files": [
    "out/**/*",
    "resources/**/*",
    "README.md",
    "CHANGELOG.md",
    "package.json",
    "node_modules/ssh2/**/*",
    "node_modules/ssh2-sftp-client/**/*",
    "node_modules/uuid/**/*",
    "node_modules/asn1/**/*",
    "node_modules/bcrypt-pbkdf/**/*"
  ]
}